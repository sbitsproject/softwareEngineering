<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
    
<navigation-rule>
   <from-view-id>/Desktop/Login.xhtml</from-view-id>
   <navigation-case>
      <from-outcome>login</from-outcome>
      <to-view-id>/Desktop/menu.xhtml</to-view-id>
      <redirect />
   </navigation-case>
</navigation-rule>

<navigation-rule>
   <from-view-id>/Desktop/menu.xhtml</from-view-id>
   <navigation-case>
      <from-outcome>signout</from-outcome>
      <to-view-id>/Desktop/Login.xhtml</to-view-id>
      <redirect />
   </navigation-case>
</navigation-rule>


<navigation-rule>
   <from-view-id>/mobile/login.xhtml</from-view-id>
   <navigation-case>
      <from-outcome>login</from-outcome>
      <to-view-id>/mobile/mobileMenu.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>invalid</from-outcome>
      <to-view-id>/mobile/login.xhtml</to-view-id>
      <redirect />
   </navigation-case>
</navigation-rule>



<navigation-rule>
   <from-view-id>/mobile/mobileMenu.xhtml</from-view-id>
   <navigation-case>
      <from-outcome>continue</from-outcome>
      <to-view-id>/mobile/displayTickets.xhtml</to-view-id>
      <redirect />
   </navigation-case>
      <navigation-case>
      <from-outcome>step1</from-outcome>
      <to-view-id>/mobile/createTicketStep1.xhtml</to-view-id>
      <redirect />
   </navigation-case>
    <navigation-case>
      <from-outcome>step3</from-outcome>
      <to-view-id>/mobile/createTicketStep3.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>step4</from-outcome>
      <to-view-id>/mobile/createTicketStep4.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>step2</from-outcome>
      <to-view-id>/mobile/createTicketStep2.xhtml</to-view-id>
      <redirect />
   </navigation-case>
</navigation-rule>

<navigation-rule>
   <from-view-id>/mobile/createTicketStep1.xhtml</from-view-id>
   <navigation-case>
      <from-outcome>continue</from-outcome>
      <to-view-id>/mobile/displayTickets.xhtml</to-view-id>
      <redirect />
   </navigation-case>
     <navigation-case>
      <from-outcome>step1</from-outcome>
      <to-view-id>/mobile/createTicketStep1.xhtml</to-view-id>
      <redirect />
   </navigation-case>
    <navigation-case>
      <from-outcome>step3</from-outcome>
      <to-view-id>/mobile/createTicketStep3.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>step4</from-outcome>
      <to-view-id>/mobile/createTicketStep4.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>step2</from-outcome>
      <to-view-id>/mobile/createTicketStep2.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   
</navigation-rule>

<navigation-rule>
   <from-view-id>/mobile/createTicketStep2.xhtml</from-view-id>
   <navigation-case>
      <from-outcome>continue</from-outcome>
      <to-view-id>/mobile/displayTickets.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>step1</from-outcome>
      <to-view-id>/mobile/createTicketStep1.xhtml</to-view-id>
      <redirect />
   </navigation-case>
    <navigation-case>
      <from-outcome>step3</from-outcome>
      <to-view-id>/mobile/createTicketStep3.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>step4</from-outcome>
      <to-view-id>/mobile/createTicketStep4.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>step2</from-outcome>
      <to-view-id>/mobile/createTicketStep2.xhtml</to-view-id>
      <redirect />
   </navigation-case>
</navigation-rule>

<navigation-rule>
   <from-view-id>/mobile/createTicketStep3.xhtml</from-view-id>
   <navigation-case>
      <from-outcome>continue</from-outcome>
      <to-view-id>/mobile/displayTickets.xhtml</to-view-id>
      <redirect />
   </navigation-case>
   <navigation-case>
      <from-outcome>step1</from-outcome>
      <to-view-id>/mobile/createTicketStep1.xhtml</to-view-id>
      <redirect />
   </navigation-case>
    <navigation-case>
