   </navigation-case>
   <navigation-case>
      <from-outcome>step2</from-outcome>
      <to-view-id>/mobile/createTicketStep2.xhtml</to-view-id>
      <redirect />
   </navigation-case>
</navigation-rule>

<navigation-rule>
   <from-view-id>/mobile/createTicketStep4.xhtml</from-view-id>
   <navigation-case>
      <from-outcome>continue</from-outcome>
      <to-view-id>/mobile/displayTickets.xhtml</to-view-id>
